component Library(materialSets []*mui.MaterialSet)
	h1.mountable Library
	
	each set in materialSets
		MaterialSet(set)
	
	button.mountable.action(data-action="newObject", data-trigger="click", data-type="materialset")
		svg-icon(name="plus")

component MaterialSet(set *mui.MaterialSet)
	h2= set.Name

	each material in set.Materials()
		Material(material)

component Material(material *mui.Material)
	h3= material.Name